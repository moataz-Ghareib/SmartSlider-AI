# ===== Frontend Environment Variables (VITE_*) =====
# متغيرات مرئية في المتصفح - تأكد من أنها آمنة

# Google Services (اختياري)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# إعدادات التطبيق
VITE_APP_NAME=SmartStartAI
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_VOICE=true

# إعدادات الأمان المحلية
VITE_ENCRYPTION_KEY=smartstart-ai-default-key-2025

# ===== Local Storage Configuration =====
# جميع البيانات محفوظة محلياً في المتصفح
# لا حاجة لقاعدة بيانات خارجية

# ===== Voice Features =====
# ميزات الصوت تعمل محلياً
VITE_VOICE_ENABLED=true
VITE_VOICE_LANGUAGE=ar-SA

# ===== Analytics (اختياري) =====
# يمكن إضافة Google Analytics إذا أردت
VITE_ANALYTICS_ENABLED=false
